{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "js/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  